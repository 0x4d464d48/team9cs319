<button ng-click="tableCtrl.addRow()">ADD A ROW!</button>
<button ng-click="tableCtrl.deleteRow()">REMOVE A ROW!</button>
<button ng-click="tableCtrl.deleteRows()">REMOVE ALL ROWS!</button>
<table border="1" cellpadding="10">
  <tr ng-show="tableCtrl.tableData.length === 0">
    <td>I'M EMPTY! FILL ME!</td>
  </tr>
  <tr ng-repeat="tableItem in tableCtrl.tableData">
    <td>I WAS CLICKED ON: {{ tableItem.timestamp; }}</td>
    <td>I AM DATA ITEM NUMBER {{ tableItem.totalItemsMade; }}!</td>
  </tr>
</table>

